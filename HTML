            };
            if (values.etoileCarac5 <= 3) {
                setAttrs({ bonusCarac5: 0 });
            };
            if (values.etoileCarac5 == 4) {
                setAttrs({ bonusCarac5: 1 });
            };
            if (values.etoileCarac5 == 5) {
                setAttrs({ bonusCarac5: 2 });
            };
        });
    });
</script>
<!-- *********************************************************************** -->
<script type="text/worker">
    on("change:valeurCarac6", function() {
        getAttrs(["etoileCarac6","valeurCarac6"], function(values) {
            if (values.etoileCarac6 !== values.valeurCarac6) {
                setAttrs({ etoileCarac6: values.valeurCarac6 });
            };
            if (values.etoileCarac6 <= 3) {
                setAttrs({ bonusCarac6: 0 });
            };
            if (values.etoileCarac6 == 4) {
                setAttrs({ bonusCarac6: 1 });
            };
            if (values.etoileCarac6 == 5) {
                setAttrs({ bonusCarac6: 2 });
            };
        });
    });
</script>
