<script type="text/worker">
    on("change:valeurCarac1", function() {
        getAttrs(["etoileCarac1","valeurCarac1"], function(values) {
            if (values.etoileCarac1 !== values.valeurCarac1) {
                setAttrs({ etoileCarac1: values.valeurCarac1 });
            };
            if (values.etoileCarac1 <= 3) {
                setAttrs({ bonusCarac1: 0 });
            };
            if (values.etoileCarac1 == 4) {
                setAttrs({ bonusCarac1: 1 });
            };
            if (values.etoileCarac1 == 5) {
                setAttrs({ bonusCarac1: 2 });
            };
        });
    });
</script>
